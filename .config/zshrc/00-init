export EDITOR=nvim
export PATH="/usr/lib/ccache/bin/:$PATH"
export ZSH="$HOME/.oh-my-zsh"

eval "$(zoxide init zsh --cmd cd)"

# fnm
FNM_PATH="/home/siwei/.local/share/fnm"
if [ -d "$FNM_PATH" ]; then
  export PATH="/home/siwei/.local/share/fnm:$PATH"
  eval "`fnm env`"
fi
